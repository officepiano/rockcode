		float: left;
		display: inline-block;
	}

	.light-theme a {
		font: 1.1em Georgia;
		font-weight: normal;
		line-height: 2em;
		float: left;
		padding: 0 6px;
		margin: 0 3px 0 0;
		min-width: 14px;
		color: #666;
		text-align: center;
		display: inline-block;
	}

	.light-theme span {
		font: 1.1em Georgia;
		font-weight: normal;
		line-height: 2em;
		float: left;
		padding: 0 6px;
		margin: 0 3px 0 0;
		min-width: 14px;
		color: #666;
		text-align: center;
	}

	.light-theme a.prev, .light-theme  a.next {
		font-size:14px;
		font-weight: normal;
		line-height:24px;		
		float: left;
		padding: 0;
		margin: 0 5px 0 0;
		width: 36px;
		height: 36px;
		color: #666;
		text-align: center;
		text-indent: -9999px;
	}
	.light-theme a.prev {
		background: url(../images/prev_pagination.png) no-repeat;
	}
	.light-theme a.next {
		background: url(../images/next_pagination.png) no-repeat;
	}

	.light-theme a {
		text-decoration: none;
	}
	.light-theme a:hover {
		text-decoration: none;
		color: #33d3a7;
	}

	.light-theme .current {
		color: #33d3a7;
		box-shadow: none;
	}

	.light-theme .ellipse {
