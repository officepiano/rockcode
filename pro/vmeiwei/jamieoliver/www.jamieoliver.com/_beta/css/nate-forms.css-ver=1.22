.site-form-N8 {width:94%!important; margin:30px 0}	
.site-form-N8 ul, .site-form-N8 li {display:block;width:100%; margin-bottom:15px}
.site-form-N8 label {font-family: Georgia;font-size:16px;font-weight:normal;}
.site-form-N8  input {height:20px!important; width:100%!important; padding:8px!important;}
.site-form-N8  textarea {width:100%!important; padding:8px!important;}

.site-form-N8  input, .site-form-N8   textarea { 
	border:1px solid #ccc;
	border-radius:5px;
	padding-right:0px;
	-moz-transition: padding .25s; 
	-webkit-transition: padding .25s; 
	-o-transition: padding .25s;
	transition: padding .25s;
	font-family: georgia;
	font-size:14px;
	letter-spacing:1px;
	color:#b3a9a9;
	width:100%;
	}

.site-form-N8 input#submit {
	width:150px!important;
	text-align:center;
	text-transform:uppercase;
	letter-spacing:0;
	display:block;
	border:0;
	font-family: courier, monospace;
	font-weight:normal!important;
	background: url('../images/forms/btn-red.png') top left no-repeat;
	color:#fff;
	font-size:12px;
	height:29px!important;
	float:right;
	padding:0!important;
	margin-right:-23px;
	margin-top:10px;
}
.site-form-N8 input#submit:hover {background-position: left -29px; color: #fff;}
.site-form-N8 input#submit:active  {background-position: left -58px;color:#fff!important;}
.site-form-N8 .half {width:100%;float:none;margin-right:0%!important;}
.site-form-N8 .half.no-margin, .site-form-N8 select.no-margin  {margin-right:0!important;}
.site-form-N8 .quarter { width:100%; float:left; margin-right:0px!important; }
.site-form-N8 .quarter.no-margin { margin-right:0px!important; }
.site-form-N8 .short-input { width:100%!important; float:none; padding-right:0px!important; margin-right:0px; }
	
.site-form-N8	input[type="file"]::-webkit-file-upload-button {
	-webkit-appearance: none;
	float: left;
	margin: 0 0 0 2px;
	border: 1px solid #AAA;
	border-radius: 4px;
	font-family: courier, monospace;
	font-size: 11px;
	text-transform: uppercase;
	background: #fff!important;
	}
	
.site-form-N8 .form input.checkbox { display:inline; width:25px!important; float:left; margin-top:3px; }


