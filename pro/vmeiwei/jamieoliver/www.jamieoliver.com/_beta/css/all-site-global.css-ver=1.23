	/* default slider style doesn't effect any style*/
	.flexslider { 
		margin: 0; 
		padding: 0;
		zoom: 1;
	}
	
	.flexslider .slides > li {
		display: none; 
		-webkit-backface-visibility: hidden;
	} 
	/* Hide the slides before the JS is loaded. Avoids image jumping */
	.flexslider .slides img {
		width: 100%; 
		display: block;
	}
	*+html .flexslider .slides img {
		width: auto;
	}
	
	/* Clearfix for the .slides element */
	.slides:after {
		content: "."; 
		display: block; 
		clear: both; 
		visibility: hidden; 
		line-height: 0; 
		height: 0;
	}
	
	html[xmlns] .slides {
		display: block;
	} 
	* html .slides {
		height: 1%;
	}
	
	/*.no-js .slides > li:first-child {display: block;}*/
	.flex-viewport {
		max-height: 2000px; 
		-webkit-transition: all 1s ease; 
		-moz-transition: all 1s ease; 
		transition: all 1s ease;
	}
	
	.loading .flex-viewport {
		max-height: 300px;
	}
	
	.flexslider .slides {
		zoom: 1;
	}
	
	/* Direction Nav */
	.flex-direction-nav a {
		text-indent: -9999px;
		opacity: 0;
	}
	
	/* Control Nav */
	.flex-control-nav {
		width: 100%; 
		position: absolute; 
		bottom: -40px;
		text-align: center;
		z-index: 1000;
	}
	
	*+html .flex-control-nav {
		left: 0px;
	}
	
	.flex-control-nav li {
		margin: 0 6px; 
		display: inline-block; 
		zoom: 1; 
		*display: inline;
	}
	.flex-control-paging li a {
		width: 11px; 
		height: 11px; 
		display: block; 
		background: #666; 
		background: rgba(102,102,102,0.8); 
		cursor: pointer; 
		text-indent: -9999px; 
		-webkit-border-radius: 20px; 
		-moz-border-radius: 20px; 
		-o-border-radius: 20px; 
		border-radius: 20px; 
		box-shadow: inset 0 0 3px rgba(0,0,0,0.3);
	}
	.flex-control-paging li a:hover { 
		background: #ccc; 
		background: rgba(204,204,204,0.7);
	}
	.flex-control-paging li a.flex-active { 
		background: #33d3a7; 
		background: rgba(51,211,167,0.9); 
		cursor: default; 
	}
	
	ul.flex-direction-nav {
		display: none;
	}
	
	#content.promotions a.button.absolute_btn {position: absolute; z-index: 100; bottom: 65px;}
	#content.promotions img.absolute_img  {position: absolute; z-index: 99;}
	
	
	


