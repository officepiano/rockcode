#mobheader {
	background: url("http://cdn.jamieoliver.com/_beta/images/mob/header_icons/mob_logo.gif") no-repeat center center #4e4e4d;
	height: 49px;
	border-bottom: 1px solid #333;
}

#left-toggle {
	background: url("http://cdn.jamieoliver.com/_beta/images/mob/header_icons/mob_list.gif") no-repeat center center #4e4e4d;
	display: block;
	width: 40px;
	height: 49px;
	top: 0;
	left: 0;
	border-right: 1px solid #333;
	text-indent: -3333px;
	position: absolute;
	overflow: hidden; 
}

#right-toggle {
	background: url("http://cdn.jamieoliver.com/_beta/images/mob/header_icons/mob_search.gif") no-repeat center center #4e4e4d;
	display: block;
	width: 40px;
	height: 49px;
	top: 0; right: 0;
	border-left: 1px solid #333;
	text-indent: -3333px;
	position: absolute;
	overflow: hidden; 
}

#mobnav {
	width: 200px;
	padding: 20px;
	background: #4e4e4d;
	top: 50px; left: 0;
	position: absolute;
	z-index: 999999;
}

#mobnav a {
	color: #FFF;
	text-decoration: none;
	line-height: 30px;
	font-size: 11px;
}

#mobsearch {
	background: #4e4e4d;
	width: 100%;
	height: 80px;
	top: 50px;
	position: absolute;
	text-align: center;
	z-index: 999999;
}

#mobsearch #global_search {
	background: #FFF;
	top: 20px;
	left: 50%;
}

